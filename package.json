{
  "name": "react-downscreen",
  "description": "React Downscreen ⛹️",
  "version": "0.3.1",
  "repository": "github:therealparmesh/react-downscreen",
  "author": "Parmesh Krishen <therealparmesh@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build": "pika build",
    "docz": "docz build",
    "pre-commit": "tsc --noEmit && pretty-quick --staged 'src/**/*.ts*'",
    "publish": "pika publish",
    "start": "docz dev"
  },
  "dependencies": {
    "scroll-into-view-if-needed": "2.2.20"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.0",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "7.7.0",
    "@babel/preset-typescript": "7.7.2",
    "@pika/pack": "0.5.0",
    "@pika/plugin-build-node": "0.7.1",
    "@pika/plugin-build-types": "0.7.1",
    "@pika/plugin-build-web": "0.7.1",
    "@pika/plugin-standard-pkg": "0.7.1",
    "@types/react": "16.9.11",
    "@types/styled-components": "4.1.21",
    "docz": "1.3.2",
    "docz-theme-default": "1.2.0",
    "husky": "3.0.9",
    "prettier": "1.18.2",
    "pretty-quick": "2.0.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "styled-components": "4.4.1",
    "typescript": "3.7.2"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run pre-commit"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-typescript",
      "@babel/preset-react"
    ]
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  }
}
